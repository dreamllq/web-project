{
  "name": "@app/backend",
  "version": "0.0.1",
  "private": true,
  "description": "NestJS backend service",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "tsx watch src/main.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "pnpm typeorm migration:generate -d src/database/data-source.ts",
    "migration:run": "pnpm typeorm migration:run -d src/database/data-source.ts",
    "migration:revert": "pnpm typeorm migration:revert -d src/database/data-source.ts",
    "migration:show": "pnpm typeorm migration:show -d src/database/data-source.ts"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^11.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/config": "^4.0.3",
    "@nestjs/typeorm": "^11.0.0",
    "cache-manager": "^7.2.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.3.1",
    "ioredis": "^5.9.3",
    "pg": "^8.18.0",
    "typeorm": "^0.3.28",
    "uuid": "^13.0.0"
  }
}
